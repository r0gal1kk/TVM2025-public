  "main": "out/index.js",
  "type": "commonjs",
	"files": [
		"out/**/*.js",
		"out/**/*.d.ts"
	],
  "scripts": {
    "moveJs": "cp src/*.ohm-bundle.js out || copy src\\*.ohm-bundle.js out",
    "copyTs": "cp src/*.ohm-bundle.d.ts out || copy src\\*.ohm-bundle.d.ts out",
    "build": "pnpm ohm generateBundles --withTypes src/*.ohm && tsc --build && pnpm moveJs && pnpm copyTs ",
    "test": "pnpm build && pnpm jest . -c ../../jest.config.js",
    "ohm": "ohm generateBundles lab*/src/*.ohm"
  },
